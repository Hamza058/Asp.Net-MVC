@model List<Visitor>

<table class="table table-primary">
    <thead>
        <tr>
            <th>Id</th>
            <th>Ad</th>
            <th>Yorum</th>
            <th>Tarih</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Commment</td>
                <td>@item.Created</td>
            </tr>
        }
    </tbody>
</table>